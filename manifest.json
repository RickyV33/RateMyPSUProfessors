{
  "manifest_version": 1,
  "name:": "RMP-PSU",
  "version": "1.0",

  "default_locale": "en",
  "author": "Ricardo Valencia",
  "description": "An extension that grabs a professors ratings from RateMyProfessor.com and displays them as you search for classes.",
  "icons": {

  },

  "permissions": [
    "https://banweb.pdx.edu/pls/oprd/bwskfcls.P_GetCrse",
    "http://banweb.pdx.edu/pls/oprd/bwskfcls.P_GetCrse"
  ],

  "page_action": {
    "default_title": "RMP-PSU",
    "default_icon": "path.jpg",
    "default_popup": "path.html"
  },

  "background": {
    "scripts": ["bootstrap.js"],
    "persistent": false
  }
}